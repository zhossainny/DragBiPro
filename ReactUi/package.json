{
  "name": "qd-platform-ui",
  "version": "0.2.0",
  "description": "Web front-end for QD Reports",
  "scripts": {
    "start": "npm-run-all --parallel build open:src",
    "dist": "npm build-prod & npm test",
    "build": "webpack --config ./config/webpack.config.dev.js --watch --env.APP_VERSION=1.2.3",
    "build-prod": "webpack --config ./config/webpack.config.prod.js",
    "open:src": "node ./server/server.js",
    "lint": "node_modules/.bin/esw ./config/webpack.config.*.js src",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "jest": {
    "globals": {
      "APP_VERSION": "1.0.0"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTest.js",
      "jest-canvas-mock"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/test/",
      "<rootDir>/node_modules/",
      "<rootDir>/package-lock.json",
      "<rootDir>/package.json",
      "<rootDir>/libs/",
      "<rootDir>/src/configuration/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpeg|jpg|png|svg)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "testResultsProcessor": "jest-teamcity-reporter"
  },
  "author": "Zahid Hossain",
  "license": "",
